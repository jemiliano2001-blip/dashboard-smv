{
  "name": "tv-dashboard-next",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "cross-env HOST=local vite",
    "dev:ip": "cross-env HOST=network vite",
    "build": "vite build",
    "preview": "vite preview",
    "preview:local": "cross-env HOST=local vite preview",
    "preview:ip": "cross-env HOST=network vite preview",
    "serve:prod": "vite preview --host --port 3000",
    "serve:prod:https": "serve -s dist -l 3000 --ssl-cert certs/localhost.crt --ssl-key certs/localhost.key",
    "generate-cert": "node scripts/generate-cert.js",
    "deploy:storage": "node scripts/deploy-storage.js",
    "upload:storage": "npm run build && node scripts/deploy-storage.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-table": "^8.21.3",
    "@types/react-window": "^1.8.8",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.5",
    "lucide-react": "^0.303.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.3",
    "react-window": "^2.2.5",
    "recharts": "^3.6.0",
    "xlsx-js-style": "^1.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.32",
    "serve": "^14.2.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.8",
    "vitest": "^4.0.17"
  }
}
